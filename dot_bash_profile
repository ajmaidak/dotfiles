#
# Hello there!  This is Alex Maidak's .bash_profile!
#

if [[ -f /opt/dev/dev.sh ]]; then source /opt/dev/dev.sh; fi

# include .bashrc if it exists
if [ -f "$HOME/.bashrc" ]; then
  . "$HOME/.bashrc"
fi

# set PATH so it includes user's private bin if it exists
if [ -d "$HOME/bin" ] ; then
    PATH="$HOME/bin:$PATH"
fi
